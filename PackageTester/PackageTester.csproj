<Project Sdk="Microsoft.NET.Sdk">

    <!--    <Import Project="$(MSBuildThisFileDirectory)FirebirdSQL.Embedded.V5.NativeAssets.Linux.targets"/>-->

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net9.0</TargetFramework>
        <LangVersion>latestmajor</LangVersion>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <RuntimeIdentifiers>linux-arm;linux-x64</RuntimeIdentifiers>
        <!--        <RuntimeIdentifier>linux-arm</RuntimeIdentifier>-->
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
        <OutputPath>bin\Debug</OutputPath>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="FirebirdSql.Data.FirebirdClient" Version="10.3.2"/>
        <PackageReference Include="FirebirdDb.Embedded.V3.NativeAssets.Linux.All" Version="1.0.1"/>
        <PackageReference Include="FirebirdDb.Embedded.V4.NativeAssets.Linux.All" Version="1.0.1"/>
        <!--        <PackageReference Include="FirebirdSQL.Embedded.NativeAssetManager" Version="1.0.0" />-->
        <PackageReference Include="FirebirdDb.Embedded.V5.NativeAssets.Linux.All" Version="1.0.1"/>
    </ItemGroup>

</Project>
